<div aria-valuemin="0" aria-valuemax="100" {{bind-attr aria-valuenow=value class=":progress-bar striped:progress-bar-striped active:active themeClassName" style=styleString}} role="progressbar">
    {{#if label}}
        {{value}}%
    {{else}}
        <span class="sr-only">{{value}}% Complete</span>
    {{/if}}
</div>
